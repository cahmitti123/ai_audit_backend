{
  "name": "ai-audit-system",
  "version": "2.3.0",
  "description": "**REST API for AI-powered audit system with GPT-5 and Prisma.**",
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "setup.js",
  "scripts": {
    "start": "tsx src/server.ts",
    "dev": "tsx watch src/server.ts",
    "inngest": "npx inngest-cli@latest dev -u http://localhost:3002/api/inngest",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration:hint": "node -e \"console.log('Run integration tests with: $env:RUN_INTEGRATION_TESTS=1; $env:INTEGRATION_SALES_DATE=YYYY-MM-DD; npm test (see docs/testing.md)')\"",
    "build": "tsc",
    "setup": "node setup.js",
    "seed": "tsx seed.ts",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.0.0",
    "@prisma/client": "^5.20.0",
    "ai": "^4.0.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "inngest": "^3.44.3",
    "openai": "^4.77.3",
    "redis": "^4.7.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "prisma": "^5.20.0",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.15"
  }
}
